<%= simple_form_for(@destination) do |f| %>
  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :longitude %>
    <%= f.input :latitude %>
    <%= f.association :pet %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
    <%= f.button :button, 'Cancel', { :type => 'reset', :class=>"btn btn-default" } %>
  </div>
<% end %>
